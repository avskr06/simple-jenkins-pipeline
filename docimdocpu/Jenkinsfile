pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'alpine:3.18.4' }
      }
      steps {
        sh 'alpine -v'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
      stage('Mem-cache') {
      agent {
        docker { image 'redis:alpine3.18' }
      }
      steps {
        sh 'redis-cli INFO server'
      }
    }
  }
}
}
